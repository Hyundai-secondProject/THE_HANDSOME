<mapper namespace="com.kosa.mapper.MemberMapper">
	
	<insert id="join">
		insert into member 
		values(#{mid}, #{mpassword}, #{mname}, #{memail}, #{mphone}, #{mtel}, 
		#{mzipcode}, #{madderess1}, #{madderss2}, #{mbirth}, #{mgender}, #{mrefid},
		#{mlogintype}, #{mtosno}, #{menabled}, #{mrole}, #{mgrade}, #{mmileage})
	</insert>
	
	<select id="fingById" resultType="com.kosa.domain.member.MemberVO">
		select * from member
		where mid = #{mid}
	</select>
</mapper>