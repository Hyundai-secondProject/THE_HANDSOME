function showCkout(self){
    if($(self).val() == 'KO001' || $(self).val() == 'KO001-2' || $(self).val() == 'KO006' || $(self).val() == 'KO007' || $(self).val() == 'KO008' ){ 
        $('#ckout_wrap').hide();
        $('#ckout_wrap2').hide();
        $('#NEWckout_wrap2').hide();
        $('#ckout_wrap0').hide();
    }else if($(self).val() == 'KO001-4'){
    	var nowDate = new Date();
    	var NewDate = new Date('2022-06-30 10:00:00');
    	if(nowDate.getTime() <= NewDate.getTime()){
    	$('#ckout_wrap').hide();
    	$('#ckout_wrap2').show();
    	$('#ckout_wrap0').hide();
    	}else{
    	$('#ckout_wrap').hide();
        $('#NEWckout_wrap2').show();
        $('#ckout_wrap0').hide();
    	}
    }else if($(self).val() == 'KO001-3'){
        $('#ckout_wrap').hide();
        $('#ckout_wrap2').hide();
        $('#NEWckout_wrap2').hide();
        $('#ckout_wrap0').show();
    }else{
        
        $('#ckout_wrap').show();
        $('#ckout_wrap2').hide();
        $('#NEWckout_wrap2').hide();
        $('#ckout_wrap0').hide();
        
    }
    
    if($(self).val() == 'KO003'){
        $("#virtualText").show();
    }else{
        $("#virtualText").hide();
    }
    
    if($(self).val() == 'KO006'){
    	$('#paytype').val('LGCNS');
    }else{
    	$('#paytype').val('Inicis');
    }
    if(typeof fn_updateDiscInfo === "function"){
        fn_updateDiscInfo();
    }
}

